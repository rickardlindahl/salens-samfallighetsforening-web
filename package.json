  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@payloadcms/graphql": "3.0.0-beta.55",
    "@types/node": "^20.14.9",
    "@types/react": "npm:types-react@19.0.0-beta.2",
    "@types/react-dom": "npm:types-react-dom@19.0.0-beta.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.0-rc.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "5.5.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-beta.2",
      "@types/react-dom": "npm:types-react-dom@19.0.0-beta.2"
